# Calling ship xenserver to complete the cobbler kickstart

# bg install process in order to return status within timeout
#if $power_mode == 'on'
/usr/local/mothership/ship xen -i $name &
#end if

# Make sure that if vm not running, the exit code is 0
#if $power_mode == 'off'
/usr/local/mothership/ship xen -d $name -f
#end if

